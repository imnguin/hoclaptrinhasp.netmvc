<div class="prs_contact_form_main_wrapper">
    <div class="container">
        <div class="row">
            <div class="col-lg-8 col-md-8 col-sm-12 col-xs-12">
                <div class="prs_contact_left_wrapper">
                    <h2>Cập Nhật Tài Khoản</h2>
                </div>
                <div class="row">
                    <form action="/Home/ConfirmDoiMK" method="post">
                        <div class="col-lg-6 col-md-6 col-sm-12 col-xs-12">
                            <div class="prs_contact_input_wrapper">
                                @ViewBag.Message
                                <input name="MatKhauCu" type="text" class="require" placeholder="Mật khẩu cũ">
                            </div>
                        </div>
                        <div class="col-lg-6 col-md-6 col-sm-12 col-xs-12">
                            <div class="prs_contact_input_wrapper">
                                @ViewBag.Message
                                <input name="MatKhauMoi" type="text" class="require" placeholder="Mật khẩu mới">
                            </div>
                        </div>
                        <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                            <div class="response"></div>
                            <div class="prs_contact_input_wrapper prs_contact_input_wrapper2">
                                <ul>
                                    <li>
                                        <button type="submit" class="submitForm">Cập Nhập</button>
                                    </li>
                                </ul>
                            </div>
                        </div>
                    </form>
                </div>
            </div>
            <div class="col-lg-4 col-md-4 col-sm-12 col-xs-12">
                <div class="prs_contact_right_section_wrapper">
                    <ul>
                        <li>
                            <a href="#"><i class="fa fa-facebook"></i> &nbsp;&nbsp;&nbsp;facebook.com/presenter</a>
                        </li>
                        <li>
                            <a href="#"><i class="fa fa-twitter"></i> &nbsp;&nbsp;&nbsp;twitter.com/presenter</a>
                        </li>
                        <li>
                            <a href="#"><i class="fa fa-vimeo"></i> &nbsp;&nbsp;&nbsp;vimeo.com/presenter</a>
                        </li>
                        <li>
                            <a href="#"><i class="fa fa-instagram"></i> &nbsp;&nbsp;&nbsp;instagram.com/presenter</a>
                        </li>
                        <li>
                            <a href="#"><i class="fa fa-youtube-play"></i> &nbsp;&nbsp;&nbsp;youtube.com/presenter</a>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- prs contact form wrapper End -->
